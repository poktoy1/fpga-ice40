
#Output leds
set_io LED[0] C6
set_io LED[1] E3
set_io LED[2] C2
set_io LED[3] A1
set_io LED[4] B4
set_io LED[5] B5
set_io LED[6] E1
set_io LED[7] B1

#Output status_led
set_io LED_STAT   B6

#Clock
set_io CLK D1

#Buttons
set_io -pullup yes  BUTTON[0]   C5  #RESET button
set_io -pullup yes  BUTTON[1]   E2  #BUTTON count
